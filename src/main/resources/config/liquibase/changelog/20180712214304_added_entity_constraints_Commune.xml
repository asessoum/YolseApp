<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Commune.
    -->
    <changeSet id="20180712214304-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="utilisateurs_id"
                                 baseTableName="commune"
                                 constraintName="fk_commune_utilisateurs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="clients_id"
                                 baseTableName="commune"
                                 constraintName="fk_commune_clients_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="suivi_champs_id"
                                 baseTableName="commune"
                                 constraintName="fk_commune_suivi_champs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suivi_champs"/>

    </changeSet>
</databaseChangeLog>
