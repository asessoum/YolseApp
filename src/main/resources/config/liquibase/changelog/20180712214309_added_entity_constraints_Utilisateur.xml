<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Utilisateur.
    -->
    <changeSet id="20180712214309-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="agents_id"
                                 baseTableName="utilisateur"
                                 constraintName="fk_utilisateur_agents_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utilisateur"/>

        <addForeignKeyConstraint baseColumnNames="profiles_id"
                                 baseTableName="utilisateur"
                                 constraintName="fk_utilisateur_profiles_id"
                                 referencedColumnNames="id"
                                 referencedTableName="uti_profile"/>

        <addForeignKeyConstraint baseColumnNames="clients_id"
                                 baseTableName="utilisateur"
                                 constraintName="fk_utilisateur_clients_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="suivi_champs_id"
                                 baseTableName="utilisateur"
                                 constraintName="fk_utilisateur_suivi_champs_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suivi_champs"/>

    </changeSet>
</databaseChangeLog>
